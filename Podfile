# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Signup' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
		pod 'Firebase/Auth'
  	pod 'Firebase/Database'
	pod 'Firebase/Core'

  # Pods for Signup
		pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
pod 'TwitterKit'
pod ‘GoogleSignIn’
	pod 'GoogleMaps'
  pod 'GooglePlaces'
end
